{
    "data": {
        "filename": "GBPUSD.csv",
        "columns": [
            "Close",
            "Low",
            "High"
            
              
        ],
        "sequence_length": 50,
        "train_test_split": 0.85,
        "normalize": true
    },
    "training": {
        "epochs": 1,
        "batch_size": 32
    },
    "model": {
        "loss": "mse",
        "optimizer": "adam",
        "save_dir": "saved_models",
        "layers": [
            {
                "type": "lstm",
                "neurons": 200,
                "input_timesteps": 49,
                "input_dim": 3,
                "return_seq": true
            },
            {
                "type": "dropout",
                "rate": 0.8
            },
            {
                "type": "lstm",
                "neurons": 200,
                "return_seq": true
            },
            {
                "type": "lstm",
                "neurons": 150,
                "return_seq": false
            },
            {
                "type": "dropout",
                "rate": 0.7
            },
            {
                "type": "dense",
                "neurons": 1,
                "activation": "linear"
            }
        ]
    }
}
